{
  "name": "notion-notification-center",
  "version": "1.0.0",
  "description": "A beautiful, customizable notification center widget for Notion task management",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "node server.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "build": "vite build && cp legal.html dist/",
    "preview": "vite preview",
    "start": "vite preview --port 3000",
    "vercel-build": "vite build && cp legal.html dist/"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.0",
    "vite": "^5.0.0",
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "notion",
    "widget",
    "task-management",
    "productivity",
    "template",
    "embed"
  ],
  "author": "ClaritySync",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/claritysyncio-lgtm/notification-center-for-customers.git"
  },
  "homepage": "https://github.com/claritysyncio-lgtm/notification-center-for-customers#readme"
}